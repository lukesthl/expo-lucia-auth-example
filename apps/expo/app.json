{
  "expo": {
    "name": "expo-lucia-auth",
    "slug": "expo-lucia-auth",
    "scheme": "com.expoluciaauth.app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.expoluciaauth.app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.439381367822-h8hin5n5rondhfeb3p5shj7dkl92kcjb",
              "com.expoluciaauth.app"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.expoluciaauth.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-apple-authentication"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "be167728-cd4a-495b-997e-06f661b1f67e"
      }
    }
  }
}
